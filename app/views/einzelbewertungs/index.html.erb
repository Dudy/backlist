  </tr>

<% @einzelbewertungs.each do |einzelbewertung| %>
  <tr>
    <td><%= einzelbewertung.typ %></td>
    <td><%= einzelbewertung.wert %></td>
    <td><%= einzelbewertung.bewertung_id %></td>
